<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="yocto"
            fetch="git://git.yoctoproject.org"/>
    <remote name="github"
            fetch="https://github.com"/>
    <remote name="linaro"
            fetch="https://git.linaro.org"/>
    <remote name="agl"
            fetch="https://git.automotivelinux.org"/>

    <default remote="github"
             revision="refs/heads/master"
             dest-branch="master"/>

    <!-- core -->
    <project name="openembedded/openembedded-core"
             path="openembedded/openembedded-core"/>
    <project name="openembedded/bitbake"
             path="openembedded/bitbake"/>
    <project name="openembedded/meta-openembedded"
             path="openembedded/meta-openembedded"/>

    <project name="meta-raspberrypi"
             path="meta-raspberrypi"
             remote="yocto"/>

    <project name="Freescale/meta-fsl-arm"
             path="freescale/meta-fsl-arm"/>

    <project name="openembedded/meta-linaro"
             path="meta-linaro"
             remote="linaro"/>

    <project name="MentorEmbedded/meta-mentor"
             path="meta-mentor"/>

    <!-- GENIVI layers -->
    <project name="GENIVI/meta-ivi"
             path="GENIVI/meta-ivi"/>
    <project name="GENIVI/meta-genivi-dev"
             path="GENIVI/meta-genivi-dev"/>
    <project name="GENIVI/genivi-dev-platform"
             path="GENIVI/genivi-dev-platform"/>

    <!-- Automotive Grade Linux layers -->
    <project name="AGL/meta-agl"
             path="AGL/meta-agl"
             remote="agl"/>
    <project name="AGL/meta-agl-demo"
             path="AGL/meta-agl-demo"
             remote="agl"/>
    <project name="AGL/meta-agl-devel"
             path="AGL/meta-agl-devel"
             remote="agl"/>
    <project name="AGL/meta-agl-extra"
             path="AGL/meta-agl-extra"
             remote="agl"/>
</manifest>

